<%- include("../admin/_headerad") -%>

<h2>Add an user</h2>

<form action="/connectad/people/people_create" method="post">
  <div class="form-horizontal">

    
<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Prenom">FirstName</label>
  <div class="col-sm-7">
    <input class="form-control" name="Prenom" value="<%= model.fname %>" required/>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Nom">Name</label>
  <div class="col-sm-7">
    <input class="form-control" name="Nom" value="<%= model.sname %>" required/>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Email">Email</label>
  <div class="col-sm-7">
    <input class="form-control" name="Email" value="<%= model.emailu %>" required/>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Phone">Phone</label>
  <div class="col-sm-7">
    <input class="form-control" name="Phone" value="<%= model.phone %>" required/>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Username">Username</label>
  <div class="col-sm-7">
    <input class="form-control" name="Username" value="<%= model.username %>" required/>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Password">Password</label>
  <div class="col-sm-7">
    <input class="form-control" name="Password" value="<%= model.password %>" required/>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Role">Role</label>
  <div class="col-sm-7">
    <input class="form-control" name="Role" value="<%= model.role %>" required/>
  </div>
</div>

</div>

    
    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Ajouter" class="btn btn-default btn-success" />
        <a class="btn btn-outline-dark cancel" href="/connectad/companies">Cancel</a>
      </div>
    </div>
  </div>
</form>

<%- include("../admin/_footerad") -%>